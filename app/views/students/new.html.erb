<h1>New Student</h1>
<%= form_for :student, url: students_path do |f| %>
<p>
  <%= f.label :username %><br>
  <%= f.text_field :username %>
</p>
<p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</p>
<p>
<p>
  <%= f.label :bio %><br>
  <%= f.text_field :bio %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
